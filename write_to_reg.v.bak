module write_to_reg(
	input wire [3:0] reg_select,  //input should be given by Control unit
	output wire tp1, tp2, ac;
	);
	
	localparam TP1 = 4'd1;
	localparam TP2 = 4'd2;
	localparam AC = 4'd3;
				  
				  
				   
	assign tp1 = (reg_select == TP1)? 1'b1 : 0;
	assign tp2 = (reg_select == TP2)? 1'b1 : 0;
	assign ac = (reg_select == R3)? 1'b1 : 0;
	
	
endmodule